<h3 class='container_title' id='form_title'>Nuevo</h3>
<div class='form_container'>
    
<div class='form'>

<% form_for(@incorporado) do |f| %>
    <%= f.error_messages %>
    
        <%= render :partial => 'form_edit_new', :locals => {:f => f}%>
    
        <p>
          <%= f.submit 'Crear' %>
        </p>
    <% end %>

</div>

<div class='form_links_container'>
    <p class='inlined' id='regresar'>Regresar a:</p>
    <p class='inlined'><%= link_to 'Incorporados', incorporados_path %></p>
    <p class='inlined'><%= render :partial => 'compartidos/link_to_admin' %></p>
</div>
</div>