<% if instance_variable_get("@paquete") %>
    <%= render :partial => 'compartidos/anterior_fue_paquete', :locals => {:paquete => @paquete}%>
<% elsif instance_variable_get("@servicio") %>
    <%= render :partial => 'compartidos/anterior_fue_servicio', :locals => {:servicio => @servicio} %>
<% end %>

<% c, modelo_plural_o_singular = modelo_info("Incorporado", @incorporados.length) %>

<h3 id='listing_title_for_table' class='container_title'><%= modelo_plural_o_singular %></h3>
<br>
<table>
  <tr>
    <th class='attribute_name_table' id='a_name1'>Detalles</th>
    <th class='attribute_name_table' id='a_name2'>Costo</th>
    <th class='attribute_name_table' id='a_name1'>Vigencia</th>    
  </tr>
  
<% @incorporados.each do |incorporado| %>
  <tr>
    <td class='attribute_value_table' id='a_value1'>
        <div><%=h incorporado.detalles %></div>
        <p><%= link_to "Paquete", :controller => :paquetes, :action => :some, :id => incorporado, :tipo => "Incorporado" %></p>
        <p>
            <%= link_to "Servicio", :controller => :servicios, :action => :some, :id => incorporado, :tipo => "Incorporado" %>
        </p>
        
    </td>
    <td class='attribute_value_table' id='a_value2'>
        <div><%=h incorporado.costo_ %></div>
    </td>
    <td class='attribute_value_table' id='a_value1'>
        <div><%=h incorporado.vigencia %></div>
    </td>
    <td><%= button_to 'Mostrar', incorporado, :method => :get %></td>
    <td><%= button_to 'Modificar', edit_incorporado_path(incorporado), :method => :get  %></td>
    <td><%= button_to 'Eliminar', incorporado, :confirm => 'Â¿Eliminar registro?', :method => :delete %></td>
  </tr>
  
<% end %>
</table>

<%= will_paginate(@incorporados, :previous_label => "Anterior", :next_label => "Siguiente") %>

<%= button_to 'Nuevo incorporado', new_incorporado_path, :method => :post %>
