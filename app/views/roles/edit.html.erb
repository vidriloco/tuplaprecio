<h3 class='container_title' id='form_title'>Modificar</h3>
<div class='form_container'>

<div class='form'>
<% form_for(@rol) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :nombre %><br />
    <%= f.text_field :nombre %>
  </p>
  <p>
    <%= f.submit 'Crear' %>
  </p>
<% end %>

</div>
<%= render :partial => 'compartidos/edita_form_links', :locals => {:obj_editable => @rol, :modelo => "rol" } %>
</div>